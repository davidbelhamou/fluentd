{
  "index_patterns": ["python-logs-*"],
  "data_stream": {
    "name": "python-logs"
  },
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "strict_date_time"
        },
        "user_id": {
          "type": "long"
        },
        "action": {
          "type": "keyword"
        },
        "status": {
          "type": "keyword"
        },
        "response_time": {
          "type": "float"
        },
        "source_file": {
          "type": "keyword"
        }
      }
    }
  }
} 