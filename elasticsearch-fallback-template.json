{
  "index_patterns": ["logs-fallback*"],
  "data_stream": {
  },
  "template": {
    "settings": {
      "number_of_shards": 1,
      "number_of_replicas": 1
    },
    "mappings": {
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "strict_date_time"
        }
      }
    }
  }
}
